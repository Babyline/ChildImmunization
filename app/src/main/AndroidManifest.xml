<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.babylinechelangat.childimmunization" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChildImmunization" >
        <activity
            android:name=".common.ui.LandingActivity"
            android:exported="true"
            android:theme="@style/Theme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".common.ui.AuthActivity"
            android:exported="false"
            android:theme="@style/Theme.NoActionBar" />
        <activity
            android:name=".parent.ui.ParentDashBoardActivity"
            android:exported="false"
            android:theme="@style/Theme.ChildImmunization.NoActionBar"
            android:label="@string/title_activity_parent_dash_board" />
        <activity
            android:name=".nurse.ui.NurseDashBoardActivity"
            android:exported="false"
            android:theme="@style/Theme.ChildImmunization.NoActionBar"
            android:label="@string/title_activity_nurse_dash_board" />
    </application>

</manifest>